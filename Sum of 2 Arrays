#include <stdio.h>

int main() {
  int *arr1;
  int *arr2;
  int *sum;
  arr1=(int*)malloc(5*sizeof(int));
  arr2=(int*)malloc(5*sizeof(int));
  sum=(int*)malloc(5*sizeof(int));
  printf("Enter numbers : ");
  for(int i=0;i<5;i++)
    {
      scanf("%d",&arr1[i]);
    }
  printf("Enter numbers : ");
  for(int i=0;i<5;i++)
  {
    scanf("%d",&arr2[i]);
  }
  for(int i=0;i<5;i++)
    {
      sum[i]=arr1[i]+arr2[i];
    }
  for(int i=0;i<5;i++)
    {
      printf("%d\t",sum[i]);
    }
  return 0;
}
